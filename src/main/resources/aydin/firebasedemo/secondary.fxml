<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" styleClass="vbox" stylesheets="@../../welcomeview.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.SecondaryController">
    <children>
        <Label styleClass="secondarylabel" stylesheets="@../../welcomeview.css" text="Secondary View" />
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" prefHeight="27.0" prefWidth="179.0" styleClass="primaryview" stylesheets="@../../welcomeview.css" text="Switch to Primary View" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
